def risco_cardiaco(idade: int, imc: float) -> str:
  if idade < 45:
    if imc < 22:
      risco = "baixo"
    else:
      risco = "mÃ©dio"
  else:
    if imc < 22:
      risco = "mÃ©dio"
    else:
      risco = "alto"
  return risco

idade = int(input())
